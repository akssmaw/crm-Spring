<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot.dao.chat.ChatComMapper">

    <select id="Chat_ComByComId" resultType="com.example.springboot.entity.chat.chat_com">

        SELECT * FROM `chat_com` WHERE comId=#{comid}

    </select>
<select id="Chat_ComByComIdCheck"  resultType="int">

    SELECT COUNT(*) FROM `chat_com` WHERE comId = #{comid}

</select>
</mapper>